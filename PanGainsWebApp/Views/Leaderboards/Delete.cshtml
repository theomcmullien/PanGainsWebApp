@model PanGainsWebApp.Models.Leaderboard
@{
    ViewData["Title"] = "Leaderboard";

}

<div class="text-white delete">

    <div class="mb-3">
        <a style="color: white; text-decoration: none; font-weight: bold" asp-area="" asp-controller="Leaderboards" asp-action="Index">&lt; Back</a>
    </div>

    <div class="card-delete">

        <p class="text-delete">Remove next months challenge?</p>

        <form asp-action="Delete" autocomplete="off">
            <input type="hidden" asp-for="LeaderboardID" />
            <input type="submit" value="Remove" class="button-delete" />
        </form>
    </div>
</div>
